package com.collectionframework;

public class LinkedListOperations {

	Node head;
	
	static class Node{
		int data;
		Node next;
		
		Node(int d){
			data=d;
			next=null;
		}
	}
	
	public static void print_list(LinkedListOperations list)
	{
		Node currNode=list.head;
		
		while(currNode!=null) {
			System.out.println(currNode.data);
		}
	}
}
