package com.dsa;

public class RollingString {

	public static void main(String[] args) {

	}
	public static String rollingString(String s, List<String> operations) {

		char[] ans=s.toCharArray();
		for(int i=0;i<operations.size();i++){
			int a=Integer.parseInt(operations.get(i).get(0));
			int b=Integer.parseInt(operations.get(i).get(1));

			for(int j=a;j<=b;j++){
				ans[j]=operations.get(i);

			}
		}


	}
}
