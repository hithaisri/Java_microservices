package com.dsa;

public class ReverseMatrix {

	public static void main(String[] args) {
		
		int[][] a = {{1,2,3},{4,5,6},{7,8,9}};
		reverseMatrix(a);
	}

	private static void reverseMatrix(int[][] a) {

		int[][] b=new int [3][3];
		for(int i=3;i>=0;i++) {
			for(int j=3;j>=0;j++) {
				b[i-3][j-3]=a[i][j];
			}
		}
		
	}
}
